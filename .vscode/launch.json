    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            
        },
        {
            "name": "flutter_vs",
            "request": "launch",
            "type": "dart",
            "program": "lib/global/main.dart"
        },
        {
            "name": "flutter_vs (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/global/main.dart"
        },
        {
            "name": "flutter_vs (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/global/main.dart"
        }
    ]
